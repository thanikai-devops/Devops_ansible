@Library("my-shared-library") _

pipeline {
    node 

    environment {
        git_url = 'https://github.com/thanikai-devops/Devops_ansible.git'
        git_branch = 'dev-jenkins'
    }
    stages{
        stage("Build stage "){
            steps{
                script{
                     // Set Git user identity locally for this build
                    sh 'git config --global user.name "Your Jenkins User"'
                    sh 'git config --global user.email "your-email@example.com"'
                    // Now call the deploy function
                    deploy(dev)
                }
            }
        }
    }
}