@Library("my-shared-library")_

pipeline {
    agent any 

    environment {
        git_url = 'https://github.com/thanikai-devops/Devops_ansible.git'
        git_branch = 'dev-jenkins'
    }
    stages{
        stage("Build stage "){
            steps{
                script{
                    sh '''
                        git config --global user.name "thanikai-devops"
                        git config --global user.email "thanikaichandran.cj@sciflare.com"
                    '''
                    deploy(dev)
                }
            }
        }
    }
}