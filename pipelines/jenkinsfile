@Library('my-shared-library') _

pipeline {
    agent any 

    environment {
        git_url = 'https://github.com/thanikai-devops/Devops_ansible.git'
        git_branch = 'dev-jenkins'
    }
    stages{
        stage("Checkout"){
            steps{
                script{
                    git url: git_url , branch: git_branch
                }
            }
        }
        stage("Hello World") {
            steps{
                script{
                    deploy("dev")
                }
            }
        }
        stage("Docker"){
            steps{
                script{
                    docker()
                }
            }
        }
    }
}